<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
<class name="com.js.oa.hr.kq.po.KqLeavePO" table="kq_leave" >
<id name="id" column="ID" type="long">
<generator class="native" />
</id>
<property name="userId" column="USER_ID" />
<property name="userName" column="USER_NAME" />
<property name="orgId" column="ORG_ID" />
<property name="orgName" column="ORG_NAME" />
<property name="startTime" column="STARTTIME" />
<property name="endTime" column="ENDTIME" />
<property name="cause" column="CAUSE" />
<property name="status" column="STATUS" />
<property name="leaveType" column="LEAVE_TYPE" />
</class>
</hibernate-mapping>
