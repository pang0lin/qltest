<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="com.js.system.vo.rssmanager.ItemStateVO" table="org_rss_item_state">
		<id name="stateId" column="state_id" type="long">
			<generator class="native" />
		</id>
		<property name="itemId" column="item_id"/>
        <property name="userId" column="user_id"/>
        <property name="readState" column="read_state"/>
        <property name="string1" column="string1"/>
		<property name="string2" column="string2"/>
	</class>
</hibernate-mapping>
