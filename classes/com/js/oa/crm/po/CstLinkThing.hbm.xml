<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="com.js.oa.crm.po.CstLinkThing" table="cst_link_t">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="cstId" type="java.lang.String">
			<column name="cst_id" />
		</property>
		
		<property name="importThing" type="java.lang.String">
			<column name="import_thing" />
		</property>
		<property name="time" type="java.lang.String">
			<column name="time" />
		</property>
		<property name="descThing" type="java.lang.String">
			<column name="desc_thing" />
		</property>

	</class>
</hibernate-mapping>