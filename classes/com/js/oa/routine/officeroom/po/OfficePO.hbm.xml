<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
    <class name="com.js.oa.routine.officeroom.po.OfficePO" table="OA_OFFICE" >
        <id name="officeId" column="OFFICE_ID">
             <generator class="native"/>
        </id>
        <property name="officename" column="OFFICENAME" type="java.lang.String"/>
        
		<property name="officenumber" column="OFFICENUMBER" type="java.lang.String"/>
		<property name="officearea" column="OFFICEAREA" type="java.lang.Long"/>
		<property name="officefaces" column="OFFICEFACES" type="java.lang.String"/>
		
		<property name="buidId" column="BUIDID" type="java.lang.Long"/>
