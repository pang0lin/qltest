<property name="festiveYear" column="FestiveYear" />
<property name="festiveMonth" column="FestiveMonth" />
<property name="festiveDay" column="FestiveDay" />
<property name="festivalName" column="FESTIVALNAME"  />
<property name="festivalRemind" column="FESTIVALREMIND"  />
<property name="festivalWish" column="FESTIVALWISH"  />
<property name="festivalWishCard" column="FESTIVALWISHCARD" type="net.sf.hibernate.type.TextType" />
<property name="createdOrg" column="CreatedOrg"  />
<property name="createdEmp" column="CreatedEmp"  />
<property name="useRange" column="UseRange"  />
<property name="userRangeName" column="userRangeName"  />

