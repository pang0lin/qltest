<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
<class name="com.js.oa.security.log.po.LogModulePO" table="SEC_LOG_MODULE" >
<id name="moduleId" column="MODULE_ID" type="java.lang.Long">
	<generator class="native" />
</id>
<property name="moduleSerial" column="MODULE_SERIAL"/>
<property name="moduleName" column="MODULE_NAME"/>
<property name="moduleLog" column="MODULE_LOG"/>
<property name="domainId" column="DOMAIN_ID"/>
<property name="moduleLevel" column="MODULE_LEVEL"/>
<property name="parentSerial" column="PARENT_SERIAL"/>

</class>
</hibernate-mapping>


