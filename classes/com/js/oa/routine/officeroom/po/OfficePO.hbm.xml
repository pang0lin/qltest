		<property name="buidnumber" column="BUIDNUMBER" type="java.lang.String"/>
		<property name="officereader" column="OFFICEREADER" type="java.lang.String"/>
		
		<property name="officereadorg" column="OFFICEREADORG" type="java.lang.String"/>
		<property name="officereadgroup" column="OFFICEREADGROUP" type="java.lang.String"/>
		<property name="officereadname" column="OFFICEREADNAME" type="java.lang.String"/>
		
		<property name="createdemp" column="CREATEDEMP" type="java.lang.Long"/>
		<property name="createdempname" column="CREATEDEMPNAME" type="java.lang.String"/>
		<property name="bz" column="BZ" type="java.lang.String"/>
		
		<property name="officestation" column="OFFICESTATION" type="java.lang.Long"/>
		<property name="officeAdminId" column="OFFICEADMINID" type="java.lang.String"/>
		<property name="officeAdminName" column="OFFICEADMINNAME" type="java.lang.String"/>
		<property name="officeIsUse" column="OFFICEISUSE" type="java.lang.String"/>
		
	
	</class>
</hibernate-mapping>
