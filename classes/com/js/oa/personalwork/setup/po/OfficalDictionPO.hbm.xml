<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
<class name="com.js.oa.personalwork.setup.po.OfficalDictionPO" table="OA_OFFICALDICTION" >
<id name="id" column="OFFICALDICTION_ID" type="long">
    <generator class="native" />
</id>
<property name="empId" column="EMP_ID" type="long" length="20"/>
<property name="empName" column="empName"  />
<property name="dictionContent" column="DICTIONCONTENT" type="string" length="100"/>
<property name="dictionIsShare" column="DICTIONISSHARE" type="byte" length="1"/>
<property name="domainId" column="DOMAIN_ID"/>
</class>
</hibernate-mapping>
