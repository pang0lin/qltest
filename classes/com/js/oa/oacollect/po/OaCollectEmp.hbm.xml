<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
    <class name="com.js.oa.oacollect.po.OaCollectEmp" table="oa_collect_emp">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="collectId" type="java.lang.Long">
            <column name="collect_id" />
        </property>
        <property name="empId" type="java.lang.Long">
            <column name="emp_id" />
        </property>
        <property name="empName" type="java.lang.String">
            <column name="emp_name" length="20" />
        </property>
        <property name="empStatus" type="java.lang.Long">
            <column name="emp_status" />
        </property>
        <property name="remindCount" type="java.lang.Long">
            <column name="remind_count" />
        </property>
    </class>
</hibernate-mapping>
