<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="com.js.oa.crm.po.NumType" table="cst_type_n">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" />
		</property>
		<property name="createUserId" type="java.lang.String">
			<column name="create_user_id" />
		</property>
		<property name="createUserName" type="java.lang.String">
			<column name="create_user_name" />
		</property>
		<property name="createTime" type="java.lang.String">
			<column name="create_time" />
		</property>
		
		<property name="initValue" type="java.lang.Integer">
			<column name="init_value" />
		</property>
		
		<property name="initLong" type="java.lang.Integer">
			<column name="init_long" />
		</property>
		<property name="type" type="java.lang.String">
			<column name="type" />
		</property>
		
		<property name="numDesc" type="java.lang.String">
			<column name="num_desc" />
		</property>
		
		

	</class>
</hibernate-mapping>