<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
    <class name="com.js.oa.online.po.SecurityOnlineuser" table="sec_onlineuser">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="userId" type="java.lang.Long">
            <column name="USER_ID"  />
        </property>
        <property name="sessionId" type="java.lang.String">
            <column name="SESSION_ID" length="128" />
        </property>
        <property name="userName" type="java.lang.String">
            <column name="USER_NAME" length="50" />
        </property>
        <property name="userAccount" type="java.lang.String">
            <column name="USER_ACCOUNT" length="20" />
        </property>
        <property name="userIp" type="java.lang.String">
            <column name="USER_IP" length="20" />
        </property>
        <property name="serverIp" type="java.lang.String">
            <column name="SERVER_IP" length="20" />
        </property>
        <property name="userStatus" type="java.lang.Long">
            <column name="USER_STATUS"  />
        </property>
        <property name="userLogtime" type="java.util.Date">
            <column name="USER_LOGTIME" length="19" />
        </property>
        <property name="userUpdatetime" type="java.util.Date">
            <column name="USER_UPDATETIME" length="19" />
        </property>
        <property name="domainId" type="java.lang.Integer">
            <column name="domain_id" />
        </property>
         <property name="dutyName" type="java.lang.String">
            <column name="duty_name" length="20"/>
        </property>
        <property name="orgName" type="java.lang.String">
            <column name="org_name" length="20"/>
        </property>
        <property name="ishelper" type="java.lang.Integer">
            <column name="ishelper" length="1"/>
        </property>        
    </class>
</hibernate-mapping>
