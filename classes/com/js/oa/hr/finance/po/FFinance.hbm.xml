<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
    <class name="com.js.oa.hr.finance.po.FFinance" table="f_finance">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="userAccounts" type="java.lang.String">
            <column name="user_accounts" length="100" />
        </property>
        <property name="empId" type="java.lang.Long">
            <column name="emp_id" />
        </property>
        <property name="empName" type="java.lang.String">
            <column name="emp_name" length="20" />
        </property>
        <property name="empNumber" type="java.lang.String">
            <column name="emp_number" length="20" />
        </property>
        <property name="orgId" type="java.lang.Long">
            <column name="org_id" />
        </property>
        <property name="orgName" type="java.lang.String">
            <column name="org_name" length="300" />
        </property>
        <property name="createdDate" type="java.util.Date">
            <column name="created_date" length="0" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" />
        </property>
        <property name="salaryType" type="java.lang.Long">
            <column name="salary_type" />
        </property>
        <property name="createdEmp" type="java.lang.Long">
            <column name="created_emp" />
        </property>
        <property name="createdOrg" type="java.lang.Long">
            <column name="created_org" />
        </property>
    </class>
</hibernate-mapping>
