<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
    <class name="com.js.oa.hr.finance.po.FPage" table="f_page" >
        <id name="pageId" type="java.lang.Long">
            <column name="page_id" />
            <generator class="native" />
        </id>
        <property name="pageName" type="java.lang.String">
            <column name="page_name" length="100" unique="true" />
        </property>
        <property name="pageContent" type="java.lang.String">
            <column name="page_content" />
        </property>
        <property name="pageIsUse" type="java.lang.Long">
            <column name="page_is_use" />
        </property>
        <property name="pageFile" type="java.lang.String">
            <column name="page_file" length="800" />
        </property>
        <property name="tableId" type="java.lang.Long">
            <column name="table_id" />
        </property>
        <property name="tableDesname" type="java.lang.String">
            <column name="table_desname" length="40" />
        </property>
        <property name="createDate" type="java.util.Date">
            <column name="create_date" length="0" />
        </property>
        <property name="createdEmp" type="java.lang.Long">
            <column name="created_emp" />
        </property>
        <property name="createdOrg" type="java.lang.Long">
            <column name="created_org" />
        </property>
    </class>
</hibernate-mapping>
