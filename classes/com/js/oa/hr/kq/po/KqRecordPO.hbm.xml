<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
<class name="com.js.oa.hr.kq.po.KqRecordPO" table="kq_record" >
<id name="id" column="id" type="long">
<generator class="native" />
</id>
<property name="userId" column="USER_ID" />
<property name="orgId" column="ORG_ID" />
<property name="recordTime" column="RECORD_TIME" />
<property name="recordStatus" column="RECORD_STATUS" />
<property name="recordSeq" column="RECORD_SEQ" />
<property name="dutyType" column="DUTY_TYPE" />
<property name="timeDiff" column="TIME_DIFF" />
<property name="dutyTime" column="DUTY_TIME" />
<property name="dutyName" column="duty_name" />
<property name="nosign" column="NOSIGN" />
</class>
</hibernate-mapping>
