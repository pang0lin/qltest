<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
<class name="com.js.oa.hr.officemanager.po.PostTitlePO" table="OA_POSTTITLE" >
<id name="id" column="POSTTILE_ID" type="long">
<generator class="native" />
</id>
<property name="postTitle" column="POSTTTILE" type="string" length="50"/>
<property name="postTitleSeries" column="POSTTITLESERIES" type="string" length="30"/>
<property name="domainId" column="DOMAIN_ID"/>
</class>
</hibernate-mapping>
